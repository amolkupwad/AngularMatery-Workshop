    "jest": "^24.9.0",
    "jest-createspyobj": "^1.2.2",
    "jest-preset-angular": "^7.1.1",
    "json-server": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
